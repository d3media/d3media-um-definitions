{
    "v1": {
        "hashes": {
            "email": {
                "map": ["whiteSpaceIsSpace", "trim", "toLowerCase"],
                "querystring": "e"
            },
            "address": {
                "querystring": "a",
                "complex": [{
                    "country": {
                        "map": ["iso3166alpha2", "whiteSpaceIsSpace", "trim", "toLowerCase"]
                    }
                }, {
                    "postalCode": {
                        "map": ["collapseWhiteSpace", "whiteSpaceIsSpace", "trim", "toLowerCase"]
                    }
                }, {
                    "street": {
                        "map": ["whiteSpaceIsSpace", "trim", "toLowerCase"]
                    }
                }, {
                    "number": {
                        "map": ["whiteSpaceIsSpace", "trim", "toLowerCase"]
                    }
                }]
            },
            "countryAndPostalCode": {
                "querystring": "c",
                "complex": [{
                    "country": {
                        "map": ["iso3166alpha2", "whiteSpaceIsSpace", "trim", "toLowerCase"]
                    }
                }, {
                    "postalCode": {
                        "map": ["collapseWhiteSpace", "whiteSpaceIsSpace", "trim", "toLowerCase"]
                    }
                }]
